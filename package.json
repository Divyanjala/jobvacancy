{
  "name": "job_vacancy",
  "version": "1.0.0",
  "description": "job vacancy",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist",
    "lint": "npm run prettier:check && npm run tslint",
    "lint:fix": "npm run tslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "tslint": "lb-tslint",
    "tslint:fix": "npm run tslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/__tests__\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/__tests__/**/*.js && npm run posttest",
    "docker:build": "docker build -t job_vacancy .",
    "docker:run": "docker run -p 3000:3000 -d job_vacancy",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "node .",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/authentication": "^1.0.14",
    "@loopback/boot": "^1.0.13",
    "@loopback/context": "^1.6.0",
    "@loopback/core": "^1.1.7",
    "@loopback/openapi-v3": "^1.2.2",
    "@loopback/repository": "^1.1.6",
    "@loopback/rest": "^1.6.0",
    "@loopback/rest-explorer": "^1.1.9",
    "@loopback/service-proxy": "^1.0.9",
    "@types/nodemailer": "^4.6.6",
    "@types/nodemailer-direct-transport": "^1.0.31",
    "@types/nodemailer-smtp-transport": "^2.7.4",
    "@types/passport-http-bearer": "^1.0.33",
    "bcrypt": "^3.0.4",
    "date-and-time": "^0.14.2",
    "generate-password": "^1.4.1",
    "googleapis": "^37.2.0",
    "http-errors": "^1.7.2",
    "jsonwebtoken": "^8.5.0",
    "loopback-connector-mongodb": "^4.0.0",
    "mail-promise": "^1.2.8",
    "mongoose-unique-validator": "^2.0.2",
    "nodemailer": "^5.1.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "passport": "^0.4.0",
    "passport-http": "^0.3.0",
    "passport-http-bearer": "^1.0.1",
    "simple-oauth2": "^2.2.1",
    "ts-token-generator": "^1.0.4",
    "typescript-validator": "^1.0.2"
  },
  "devDependencies": {
    "@loopback/build": "^1.3.1",
    "@loopback/testlab": "^1.0.7",
    "@loopback/tslint-config": "^2.0.1",
    "@types/node": "^10.12.29",
    "@types/passport": "^1.0.0",
    "@types/passport-http": "^0.3.7",
    "tslint": "^5.12.0",
    "typescript": "^3.3.1"
  }
}
